{
  "name": "@near-everything/marketplace-plugin",
  "version": "1.0.0",
  "description": "Unified marketplace plugin with Schema.org Product support",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "peerDependencies": {
    "every-plugin": "link:every-plugin",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@effect/language-service": "^0.41.1",
    "@module-federation/node": "^2.7.17",
    "@rspack/cli": "^1.5.2",
    "@rspack/core": "^1.5.2",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^24.6.0",
    "dotenv": "^17.2.2",
    "drizzle-kit": "^0.31.2",
    "every-plugin": "0.3.0",
    "msw": "^2.3.0",
    "sirv": "^3.0.2",
    "testcontainers": "^11.7.1",
    "vitest": "^3.2.4",
    "zephyr-rspack-plugin": "link:zephyr-rspack-plugin",
    "zod": "^4.1.12"
  },
  "scripts": {
    "build": "rspack build && tsc -p tsconfig.build.json",
    "type-check": "tsc -p tsconfig.build.json --noEmit",
    "dev": "rspack serve",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest --config vitest.integration.config.ts",
    "coverage": "vitest run --coverage",
    "db:local": "turso dev --db-file local.db",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate"
  },
  "dependencies": {
    "@libsql/client": "^0.14.0",
    "@orpc/client": "^1.9.3",
    "drizzle-orm": "^0.44.2",
    "drizzle-zod": "^0.8.3",
    "feed": "^5.1.0",
    "ioredis": "^5.8.1",
    "telegraf": "^4.16.3"
  }
}
